
<div class="booyah-box col-10 offset-1">
  <h1>create new project</h1>
    <%= @org.name %>
      <%= simple_form_for @project, url: org_projects_path(@org) do |f| %>
      <%= f.input :name %>
      <%= f.input :description %>
      <%= f.input :status %>
    <br />
      <%= f.submit 'Create', class: 'btn btn-primary' %>
    <% end %>

</div>

